/* baseui/nav-bar/index.wxss */
.nav {
  display: flex;
  font-size: 28rpx;
}
.left, .right, .center, .left-default {
  display: flex;
  justify-content: center;
  align-items: center;
}
.left, .right {
  width: 120rpx;
}
.center {
  flex: 1;
}
.slot:empty + .default {
  display: flex;
}
.default {
  display: none;
}

.left-default {
  width: 100%;
  height: 100%;
}
.left-default image{
  width: 44rpx;
  height: 44rpx;
  display: block;
}